{% extends '::base.html.twig' %}

{% block body %}
    <div class="row container-fluid" style="padding-top: 5%">
        <div class="breadcrumb-wrap mbl" >
            <div class="container relative text-left">
                <ul class="breadcrumb" id="breadcrumb_li">
                    <li><a href="{{ path('med_service_main_homepage') }}"><i class="fa fa-home fa-2x"></i></a><span></span></li>
                    <li>
                        <a href="{{ path('get_zone',{'zone':'tunisie'}) }}">Toute la tunisie</a><span></span>
                    </li>
                    <li class="active">
                        <a href="{{ route }}">{{  path }}</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="col-md-3">
            <div class="panel-blue">
                <div class="panel-title">Gouvernerat</div>
            </div>
        </div>
        <div class=" col-md-9 page-content">
            <div id="tab-general">
                <div class="row mbl">
                    <div class="col-lg-12">

                        <div class="col-md-12">
                            <div id="area-chart-spline" style="width: 100%; height: 300px; display: none;">
                            </div>
                        </div>

                    </div>

                    <div class="col-lg-12">

                        <div class="page-content">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="panel">
                                        <div class="panel-body">
                                            <div id="grid-layout-table-1" class="box jplist">
                                                <div class="box text-shadow">
                                                    <table class="demo-tbl"><!--<item>1</item>-->
                                                        {% for annonce in annonces %}
                                                            <tr class="tbl-item row"><!--<img/>-->
                                                                <td class="img col-md-3"><img src="{{ asset(annonce.images.first.path) }}" width="120px" height="90px" alt="" title=""/></td>
                                                                <!--<data></data>-->
                                                                <td class="td-block"><p class="date">03/15/2012</p>
                                                                    <p class="title text-left">{{ annonce.titre }}</p>

                                                                    <p class="desc text-left">{{ annonce.description }}</p>

                                                                    <p class="like text-left" style="margin-top: 20px">5 Likes</p></td>
                                                            </tr>
                                                        {% endfor %}
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('test/JS/jplist.js') }}"></script>
{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('test/css/jplist-custom.css') }}">
{% endblock %}

{% endblock %}

{% block javascripts %}

{% endblock %}

